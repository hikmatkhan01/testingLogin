package java2intor;

public class javaPrictice {

	public static void main(String[] args) {
		// TODO Auto-generated method stub	
		
		
        int[] arr= {1,2,3,6,8,9,10,122,55};	
        for(int a=0; a<=8;a++)
        {
        	if(arr[a] / 2==1)
        	{
        		System.out.print("divided by 2 = "+arr[a]+"\n");
        	}
        	else
        	{
        		System.out.print("\nNot divided  "+arr[a]+"\n");	
        	}
        	
        }
	
	}

}
