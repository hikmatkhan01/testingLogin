package java2intor;

public class javaPrictice {

	public static void main(String[] args) {
		// TODO Auto-generated method stub	
		System.out.println("hallo bahi");
		
		int[] arr1 = new int[5];
		arr1[0]=1;
		arr1[1]=2;
		arr1[2]=3;
		arr1[3]=4;
		arr1[4]=5;
		for(int i=0;i<=4;i++)
		{
			System.out.print(arr1[i]+"\n");	
		};
		
		
		int[] arr= {1,2,3,4,5};
		for(int i=0;i<=4; i++)
		{
			System.out.print(arr[i]);
		};
		
		String[] name= {"khan","Miakhel","Hikmat","Imran"};
		for(String s: name)
		{
			System.out.print(s);
		};
		
	
	}

}
